{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-checkbox": "cd nova-components/Checkbox && npm run dev",
        "build-checkbox-prod": "cd nova-components/Checkbox && npm run prod",
        "build-phonenumber": "cd nova-components/Phonenumber && npm run dev",
        "build-phonenumber-prod": "cd nova-components/Phonenumber && npm run prod",
        "build-dropdown": "cd nova-components/Dropdown && npm run dev",
        "build-dropdown-prod": "cd nova-components/Dropdown && npm run prod",
        "build-phone_number": "cd nova-components/PhoneNumber && npm run dev",
        "build-phone_number-prod": "cd nova-components/PhoneNumber && npm run prod",
        "build-multiselctef": "cd nova-components/Multiselctef && npm run dev",
        "build-multiselctef-prod": "cd nova-components/Multiselctef && npm run prod",
        "build-filtercustom": "cd nova-components/Filtercustom && npm run dev",
        "build-filtercustom-prod": "cd nova-components/Filtercustom && npm run prod",
        "build-filter-custom": "cd nova-components/FilterCustom && npm run dev",
        "build-filter-custom-prod": "cd nova-components/FilterCustom && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.19",
        "cross-env": "^7.0",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.19",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "vue-multiselect": "^2.1.6",
        "vue-tel-input": "^4.4.0"
    }
}